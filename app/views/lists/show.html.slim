h2 = best_in_place @list, :name, :type => :input
= link_to "delete", @list, :method => "delete"

= render :partial => 'point_form', :locals => { :pro => true, :classy => "success", :submit_title => "Pro" } 

- @list.points.ispro.each do |point|
  = render :partial => "points/point", :locals => { :point => point }

= render :partial => 'point_form', :locals => { :pro => false, :classy => "error", :submit_title => "Con" } 

- @list.points.iscon.each do |point|
  = render :partial => "points/point", :locals => { :point => point }